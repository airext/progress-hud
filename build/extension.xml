<extension xmlns="http://ns.adobe.com/air/extension/24.0">
  <id>com.github.airext.ProgressHUD</id>
  <versionNumber>0.0.1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXProgressHUD.a</nativeLibrary>
        <initializer>ANXProgressHUDInitializer</initializer>
        <finalizer>ANXProgressHUDFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libANXProgressHUDSimulator.a</nativeLibrary>
        <initializer>ANXProgressHUDInitializer</initializer>
        <finalizer>ANXProgressHUDFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXProgressHUD.jar</nativeLibrary>
        <initializer>com.github.airext.ProgressHUD</initializer>
        <finalizer>com.github.airext.ProgressHUD</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>libANXProgressHUD.jar</nativeLibrary>
        <initializer>com.github.airext.ProgressHUD</initializer>
        <finalizer>com.github.airext.ProgressHUD</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
